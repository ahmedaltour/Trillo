{
  "name": "Trillo",
  "version": "1.0.0",
  "description": "trillo app",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
  "compile:sass": "node-sass sass/main.scss css/style.comp.css",
  "build": "postcss css/style.comp.css --use autoprefixer -d css/style.prefix.css",
  "compress:css": "node-sass  css/style.prefix.css/style.comp.css css/style.css --output-style compressed",
  "build:css": "npm-run-all compile:sass build compress:css"
  },
  "author": "Jonas",
  "license": "ISC",
  "browserslist": "last 10 versions",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1"
  }
}



